{
  "name": "ts_bindings",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "ts_bindings",
    "triples": {
      "additional": [
        "aarch64-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.15",
    "@types/bun": "^1.1.11",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release && bun run src/post-process.ts",
    "test": "tsc --noEmit && bun test",
